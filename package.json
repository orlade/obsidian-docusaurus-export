    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.37",
    "jest": "^27.0.6",
    "obsidian": "^0.12.0",
    "obsimian": "../obsimian",
    "prettier": "^2.3.2",
    "rollup": "^2.32.1",
